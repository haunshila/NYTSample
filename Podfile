source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
use_frameworks!

target 'NYTSample' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  # Pods for NYTSample
    pod 'Alamofire', '~> 4.7'
    pod 'PKHUD', '~> 5.0'
    pod 'SDWebImage', '~>3.8'


end
  
target 'NYTSampleTests' do
  inherit! :search_paths
  # Pods for testing
    pod 'Alamofire', '~> 4.7'
    pod 'PKHUD', '~> 5.0'
    pod 'SDWebImage', '~>3.8'


end

target 'NYTSampleUITests' do
  inherit! :search_paths
  # Pods for testing
    pod 'Alamofire', '~> 4.7'
    pod 'PKHUD', '~> 5.0'
    pod 'SDWebImage', '~>3.8'


end


